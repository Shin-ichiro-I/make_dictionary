      </links>
      <math>
        <item>
          <alttext>G</alttext>
          <latex>G</latex>
        </item>
        <item>
          <alttext>ΔGr = ΔGro + R.T.lnK = 0</alttext>
          <latex>\Delta G_{\text{r}} = \Delta G_{\text{r}}^{o}+R\ T\ \ln K=0</latex>
        </item>
        <item>
          <alttext>ΔGro = -R.T.lnK</alttext>
          <latex>\Delta G_{\text{r}}^{\,\unicode{x26ac}} = -R\ T\ \ln K</latex>
        </item>
        <item>
          <alttext>K</alttext>
          <latex>K</latex>
        </item>
      </math>
